{
	admin off
	persist_config off
	auto_https off
	log {
		format json
	}
	servers {
		trusted_proxies static private_ranges 100.0.0.0/8
	}
}

:{$PORT:3000} {
	log {
		format json
	}

	rewrite /health /*

	root * /app/dist

	encode gzip

	@jsmodule path *.mjs
	header @jsmodule Content-Type "text/javascript; charset=utf-8"

	file_server

	try_files {path} /index.html
}
